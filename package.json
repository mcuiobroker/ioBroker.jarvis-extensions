{
  "name": "iobroker.jarvis-extensions",
  "version": "0.0.1",
  "description": "Extensions for jarvis-Adapter",
  "author": {
    "name": "mcuiobroker",
    "email": "mcuiobroker@gmail.com"
  },
  "homepage": "https://github.com/mcuiobroker/ioBroker.jarvis-extensions",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "ioBroker-adapter",
    "jarvis",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mcuiobroker/ioBroker.jarvis-extensions.git"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.2.2"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.8.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.2",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@iobroker/adapter-dev": "^1.0.1",
    "@iobroker/adapter-react": "2.2.4",
    "@iobroker/testing": "^5.0.0",
    "@material-ui/core": "^4.12.4",
    "@tsconfig/node16": "^16.1.3",
    "@types/chai": "^5.0.0",
    "@types/chai-as-promised": "^8.0.1",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.8.2",
    "@types/proxyquire": "^1.3.31",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.25",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^4.0.0",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "glob": "^9.3.5",
    "globals": "^15.11.0",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^4.4.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "typescript": "~5.6.3"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "lib/",
    "widgets/**/!(tsconfig|tsconfig.*|.eslintrc).json",
    "widgets/**/*.{html,css,png,svg,jpg,js}",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "prebuild": "rimraf admin/build",
    "build": "build-adapter react",
    "watch": "build-adapter react --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint .",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/mcuiobroker/ioBroker.jarvis-extensions/issues"
  },
  "readmeFilename": "README.md",
  "resolutions": {
    "protobufjs": "^7.2.4",
    "glob": "^9.0.0",
    "rimraf": "^4.0.0"
  }
}
